{
  "cli": {
    "version": ">= 5.9.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "env": {
        "APP_ENV": "production"
      },
      "android": {
        "buildType": "app-bundle"
      }
    },
    "production": {
      "channel": "production",
      "env": {
        "APP_ENV": "production"
      },
      "android": {
        "buildType": "app-bundle"
      }
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "channel": "development-simulator"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6745001049",
        "appleTeamId": "T8LRTJ92ND"
      },
      "android": {
        "serviceAccountKeyPath": "./path-to-your-google-service-account.json",
        "track": "production"
      }
    }
  }
}
